{
  "name": "user-auth",
  "version": "1.0.0",
  "description": "Service to provide authentication with JWT",
  "main": "index.js",
  "scripts": {
    "dev": "yarn && nodemon --watch 'src/**/*.ts' --exec 'ENV=dev ts-node -r tsconfig-paths/register' src/server.ts",
    "prebuild": "rm -rf /dist",
    "build": "tsc",
    "start": "ENV=dev node -r tsconfig-paths/register -r ts-node/register ./dist/src/server.js",
    "prod": "ENV=prod node -r ./prod-paths.js ./dist/src/server.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "posttypeorm": "prettier -w src/database/migrations",
    "test": "ENV=test jest --runInBand --forceExit --detectOpenHandles --useStderr",
    "test:cover": "yarn test --coverage",
    "test:ci": "ENV=ci jest --runInBand --forceExit --detectOpenHandles --coverage",
    "db:setup": "yarn typeorm schema:sync && yarn seed:run",
    "db:setup:test": "ENV=test yarn db:setup",
    "db:setup:ci": "ENV=ci yarn db:setup",
    "db:reset": "yarn typeorm schema:drop && yarn db:setup",
    "db:reset:test": "ENV=test yarn typeorm schema:drop && yarn db:setup:test",
    "lint": "eslint \"src/**\" --color",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js seed",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "devenginec",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "class-validator-jsonschema": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-formidable": "^1.2.0",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.0.1",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.1",
    "nodemailer-express-handlebars": "^6.0.0",
    "nodemailer-sendgrid-transport": "^0.2.0",
    "pg": "^8.9.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.10.1",
    "routing-controllers-openapi": "^4.0.0",
    "swagger-ui-express": "^4.6.1",
    "ts-jest": "^29.0.5",
    "tsconfig-paths": "^4.1.2",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.45",
    "typeorm-seeding": "^1.6.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/express-rate-limit": "^6.0.0",
    "@types/faker": "^6.6.9",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.14.1",
    "@types/nodemailer": "^6.4.7",
    "@types/redis": "^4.0.11",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.4.3",
    "jest-sonar-reporter": "^2.0.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
